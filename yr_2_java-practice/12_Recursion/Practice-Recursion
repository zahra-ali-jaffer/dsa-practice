//Iterative
// In List

public int size() {
  LinkedNode<T> cursor = front;
  int count = 0;
  while (cursor != null){
    count++;
    cursor = cursor,getNext();
  }
  return count;
}

//Recursuve
//In List

public int size() {
  return front.size();
}

//In Node
public int size() {
  if (next = null) return 1;
  else return 1 + next.size();
}
