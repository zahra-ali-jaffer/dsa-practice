//Sets
//Time implication -> O(n)

//pickRandom() method

public T pickRandom(){
  if (isEmpty()) throw new IllegalStateException("pickRandom: set empty!");
  
  // Use random module without importing it
  java.util.Random rdm = new java.util.Random();
  int choice = rdm.nextInt(count); //nextInt gives a random int between 0 and count-1
  return contents[choice];
}

//Set Examples -> Raffles, Lottery, MCQs and Student Records

//Bingo 
//1. create balls,
//2. store them all in a set,
//3. draw one ball from the set, announce it, and then
//4. repeat Step 3 *n - 1* times, *where n is total number of balls required in a game*.

public class BingoBall {
  //Letter corresponds to a numbers -> so easier to find
  private char letter;
  private int number;

  public BingoBall(int num) {
    this.number = num;
    if (num <= 15)
      letter = 'B'; 
    else if (num <= 30)
      letter = 'I';
		else if (num <= 45)
			letter = 'N';
		else if (num <= 60)
			letter = 'G';
		else letter = 'O';
  }
  public String toString() { return (letter + "" + number); }
}

//Bingo 

public class Bingo{
  public static void main(String[]args){
    final int NUM_BALLS = 75; 
    final int NUM_PULLS = 10;

    ArraySet<BingoBall> bingoSet = new ArraySet<BingoBall>();
    BingoBall ball;
    //Make NUM_BALLS balls
    for (int ballNum = 1; ballNum <= NUM_BALLS; ballNum++){
      ball = new BingoBall(ballNum);
      bingoSet.add(ball);
    }
    System.out.println("We have" + bingoSet.size()+ "balls in play!\n");
    //Pull NUM_PULLS balls
    for (int pull = 1; pull <= NUM_PULLS; pull++){
      ball = bingoSet.removeRandom();
      System.out.println(ball); // will run ball.toString() 
    }
  }
}


//java.util.Set does not have isSubset(), removeRandom(), or any of the union methods











